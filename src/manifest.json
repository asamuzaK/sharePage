{
  "applications": {
    "gecko": {
      "id": "sharePage@asamuzak.jp",
      "strict_min_version": "57.0a1"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "icon.svg#context",
    "default_popup": "html/popup.html",
    "default_title": "__MSG_extensionName__"
  },
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/sharePage",
  "icons": {
    "24": "icon.svg#context",
    "48": "icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ],
  "short_name": "__MSG_extensionShortName__",
  "version": "0.2.1"
}
